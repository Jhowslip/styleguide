{
  "name": "acessocard-styleguide",
  "version": "1.0.0",
  "description": "",
  "main": "dist/styleguide.css",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "sass-lint modules/**/*.scss -v -q",
    "build": "npm run lint && node-sass --include-path scss styleguide.scss dist/styleguide.css && node-sass --include-path scss styleguide.scss dist/styleguide.min.css --output-style compressed",
    "watch": "nodemon -e scss -x \"npm run build\"",
    "start": "browser-sync start --server --index \"docs/index.html\" --files \"dist/*.css, docs/*.html, docs/*.css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acessocard/styleguide.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acessocard/styleguide/issues"
  },
  "homepage": "https://github.com/acessocard/styleguide#readme",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "sass-lint": "^1.10.2"
  },
  "dependencies": {
    "flexboxgrid": "^6.3.1"
  }
}